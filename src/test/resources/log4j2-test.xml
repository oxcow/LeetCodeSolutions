<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="log4test">
    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="%d %p %c{1.} [%t] %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="debug">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>

</Configuration>